}
Graph hgraph;
hgraph.addPointProperty(dist,int);
hgraph.read(name[1]);
hgraph.getType() <GPU> graph;
graph=hgraph;
double t1,t2;
t1=rtclock();
//foreach(t In graph.points)reset(t,graph);
graph.points[0].dist=0;
changed=0;
foreach(t In graph.points)TC(t,graph);
t2=rtclock();
printf("\nTIME =%f\n",(t2-t1)*1000);
int count;
count=changed;
printf("\ntriangles=%d \n",count);
return;
}
