void TC(Point <GPU> p,Graph <GPU> graph){
Collection coll [Point(graph)];
int ch;
foreach(t In p.outnbrs){
Point (graph) t1;
t1=t;
if(t1.x <=p.x)continue;
coll.add(t1);
}
for(int i=0;i<coll.size;i++){
Point (graph) p2;
p2=coll[i];
foreach(t2 In p2.outnbrs){
Point (graph) t1;
t1=t2;
if(t1.x <=p2.x)continue;
for(int i=0;i<coll.size;i++){
Point (graph) p3;
p3=coll[i];
if(p3.x==t1.x)ADD(changed,1);
}
}
}
}
int main(int argc, char *name[]){
if(argc!=2){
printf("error: exec filename");
